{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "compilerOptions": {
    "composite": true,
    "lib": [
      "DOM",
      "ESNext"
    ],
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "types": [
      "vite/client"
    ],
    "noImplicitAny": true,
    "esModuleInterop": true,
    "isolatedModules": true
  },
  "include": [
    "src/preload.ts",
    "src/renderer.ts",
    "vite.*.config.ts",
    "forge.*.config.ts"
  ]
}
